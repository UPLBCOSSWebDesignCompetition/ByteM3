{
    "extends": "next/core-web-vitals",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-sparse-arrays": "warn",
        "no-console": "warn",
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/jsx-first-prop-new-line": [1, "multiline"],
        "react/jsx-max-props-per-line": [1,
            {
                "maximum": 1
            }
        ]
    }
}
